{{#each this}}
<li class="gallery__item film-card">
  <a class="gallery__link" href="">
    {{!-- <div class="film-card"> --}}
      <img
        src={{poster_path}}
        loading="lazy"
        class="film-card__image"
        alt={{original_title}}
      />
      <div class="film-card__group">
        <p class="film-card__title">{{title_}}</p>
        <div class="film-card__info">
          <p class="film-card__genre">{{genres}}</p>
          <p class="film-card__year">{{year}}</p>
          {{!-- <p class="film-card__rate">10.0</p> --}}
        </div>
      </div>
    {{!-- </div> --}}
  </a>
</li>
{{/each}}
